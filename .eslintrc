{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript"
  ],
  "rules": {
    "func-name-matching": "error",
    "no-empty-function": "off",
    "prefer-const": "error",
    "no-undef": "off",
    "vue/multi-word-component-names": "off",
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}
